import request from './request';
import qs from "qs";

/* 企业用户职称管理 */

// 新增企业用户职称信息
export function jobTitleadd(data) {
    return request({
        url: '/admin/api/v1/enterprise_management/job_title/add',
        method: 'post',
        data: data
    })
}

// 删除企业用户职称信息
export function jobTitledelete(data) {
    return request({
        url: '/admin/api/v1/enterprise_management/job_title/delete',
        method: 'post',
        data: data
    })
}

// 获取指定ID的企业用户职称信息
export function jobTitleget(data) {
    return request({
        url: '/admin/api/v1/enterprise_management/job_title/get',
        method: 'post',
        data: data
    })
}

// 列出满足条件的企业用户职称信息
export function jobTitlelist(data) {
    return request({
        url: '/admin/api/v1/enterprise_management/job_title/list',
        method: 'post',
        data: data
    })
}

// 修改企业用户职称信息
export function jobTitlemodify(data) {
    return request({
        url: '/admin/api/v1/enterprise_management/job_title/modify',
        method: 'post',
        data: data
    })
}
