import qs from "qs";

/* 企业部门管理 api  */
//列出满足条件的企业部门树
export function getListtreesdata(data) {
    return request({
        url: '/admin/api/v1/enterprise_management/depart/listTrees',
        method: 'post',
        data: data
    })
}
