/* 侧边栏样式 */
::-webkit-scrollbar {
width: 12px;
height: 12px;
}

::-webkit-scrollbar-track {
    border-radius: 5px;
    background-color: #222d50;
}

::-webkit-scrollbar-thumb {
    border-radius: 5px;
    background-color: #4f5c84;
}

