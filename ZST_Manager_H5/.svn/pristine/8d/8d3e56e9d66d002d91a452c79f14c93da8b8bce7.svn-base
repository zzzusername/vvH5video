
/* 文本展示弹窗样式  #zDialogtext */
#zDialogtext .block{
    width: 100%;
	margin-bottom: 20px;
    padding: 2px 4px;
    background: #4a567c;;
	overflow: hidden;
}
#zDialogtext .inline{
    width: 50%;
}
#zDialogtext .block li{
	float: left;
	width: 100%;
}
#zDialogtext .liL{
	width: 40%;
	text-align: right;
    float: left;
    font-size: 14px;
    color: #fff;
    padding: 0 12px 0 0;
    box-sizing: border-box;
    background: #1b274c;
    margin: 2px 0;
    line-height: 36px;
    border: 1px solid #3b4872;
}
#zDialogtext .imgCont{
    height: 40px;
    line-height: 40px;
}
#zDialogtext .imgCont img{
    margin-top: 5px;
    width: 30px;
    height: 30px;
}
#zDialogtext .reason .liL, #zDialogtext .reason .liR{
    height: 40px;
    line-height: 40px;
}
#zDialogtext .liR{
	float: left;
    width: 60%;
    -webkit-appearance: none;
    background-color: #2a3558;
    background-image: none;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    color: #eee;
    font-size: inherit;
    outline: 0;
    padding: 0 15px;
    line-height: 36px;
    margin: 2px 0;
    border: 1px solid #3b4872;
}
#zDialogtext .userBtn{
	text-align: right;
}
#zDialogtext .userBtn .sure{
    display: inline-block;
    line-height: 1;
    white-space: nowrap;
    cursor: pointer;
    color: #333;
    -webkit-appearance: none;
    text-align: center;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    outline: 0;
    margin: 0;
    -webkit-transition: .1s;
    transition: .1s;
    padding: 11px 30px;
    font-size: 14px;
    border-radius: 3px;
    background: linear-gradient(to right,#31f394, #31e4c1);
}
#zDialogtext .userBtn .close{
    display: inline-block;
    line-height: 1;
    white-space: nowrap;
    cursor: pointer;
    color: #333;
    -webkit-appearance: none;
    text-align: center;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    outline: 0;
    margin: 0;
    -webkit-transition: .1s;
    transition: .1s;
    padding: 11px 30px;
    font-size: 14px;
    border-radius: 3px;
    background: -webkit-gradient(linear, left top, left bottom, from(#fcfeff), to(#c9e7f8));
    background: linear-gradient(#fcfeff, #c9e7f8);
}
